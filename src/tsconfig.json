{
  "compilerOptions": {
    "target": "es6",
    "downlevelIteration": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "jsx": "react"
  },
  "exclude": [
    "__tests__/*"
  ]
}
